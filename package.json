{
  "name": "responsive-website",
  "version": "0.0.1",
  "description": "static responsive website built with HTML, CSS and JS",
  "main": "index.html",
  "scripts": {
    "sync": "browser-sync start --directory --server --files '*.js, *.html, *.css'",
    "sass:dev": "node-sass --watch src/scss --output dist/css",
    "sass:prod": "node-sass src/scss --output dist/css --output-style compressed",
    "wp:dev": "webpack --mode development --progress --watch",
    "wp:prod": "webpack --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gothaw/responsive-website.git"
  },
  "author": "Radoslaw Soltan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gothaw/responsive-website/issues"
  },
  "homepage": "https://github.com/gothaw/responsive-website#readme",
  "devDependencies": {
    "browser-sync": "~2.26.3",
    "node-sass": "~4.9.4",
    "webpack": "~4.23.1",
    "webpack-cli": "~3.1.2",
    "webpack-server": "~0.1.2",
    "express": "~4.16.4",
    "handlebars": "~4.0.12"
  },
  "dependencies": {
    "enquire.js": "~2.1.6"
  }
}
